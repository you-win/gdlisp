config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "GDLisp",
"language": "GDScript",
"path": "res://utils/GDLisp.gd"
}, {
"base": "Reference",
"class": "SaveDataUtil",
"language": "GDScript",
"path": "res://utils/SaveDataUtil.gd"
} ]
_global_script_class_icons={
"GDLisp": "",
"SaveDataUtil": ""
}

[application]

